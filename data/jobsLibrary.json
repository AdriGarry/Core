{
	"system": [
		{
			"cron": "*/30 * * * * *",
			"flux": { "id": "interface|hardware|runtime", "conf": { "hidden": true } }
		},
		{
			"cron": "1 * * * * *",
			"flux": { "id": "service|time|isAlarm", "conf": { "hidden": true } }
		},
		{
			"cron": "5 0 0 * * 1-5",
			"flux": { "id": "service|system|goToSleep" }
		},
		{
			"cron": "5 0 2 * * 0,6",
			"flux": { "id": "service|system|goToSleep" }
		},
		{
			"cron": "13 13 13 * * 1-6",
			"flux": [
				{ "id": "interface|tts|speak", "data": { "lg": "en", "msg": "Auto restart" } },
				{ "id": "service|system|restart", "conf": { "delay": 3 } }
			]
		},
		{
			"cron": "13 13 13 * * 0",
			"flux": [
				{ "id": "interface|tts|speak", "data": { "lg": "en", "msg": "Reset config" } },
				{ "id": "interface|runtime|reset", "data": true, "conf": { "delay": 3 } }
			]
		},
		{
			"cron": "15 15 13 * * 0",
			"flux": [
				{ "id": "interface|tts|speak", "data": { "lg": "en", "msg": "Auto reboot" } },
				{ "id": "service|system|reboot", "conf": { "delay": 3 } }
			]
		},
		{
			"cron": "0 2 0 * * 1",
			"flux": { "id": "interface|hardware|archiveLog" }
		}
	],
	"interactive": [
		{
			"cron": "0 0,30 * * * *",
			"flux": { "id": "service|time|now" }
		},
		{
			"cron": "13 0 1,13 * * *",
			"flux": { "id": "service|interaction|uneHeure" }
		},
		{
			"cron": "0 18,20,22-25 8 * * 1-5",
			"flux": { "id": "service|interaction|goToWork" }
		},
		{
			"cron": "0 15 18 * * 1-5",
			"flux": { "id": "service|music|fipOrJukebox" }
		},
		{
			"cron": "0 19 19 * * *",
			"flux": { "id": "interface|tts|speak", "data": "Je crois qu'il faut lancer l'opairation baluchon" }
		},
		{
			"cron": "13 13,25,40,51 17-21 * * *",
			"flux": { "id": "service|interaction|random" }
		}
	]
}
