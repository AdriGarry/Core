{
	"conf": {
		"mode": "ready",
		"log": "info",
		"watcher": false,
		"startTime": ".",
		"alarms": ".",
		"version": ".",
		"flux": []
	},
	"runtime": {
		"etat": null,
		"volume": null,
		"max": null,
		"mood": [],
		"music": false,
		"alarm": false,
		"timer": 0,
		"voicemail": 0,
		"audioRecord": 0,
		"hdmi": null,
		"screen": null,
		"cpu": {
			"usage": null,
			"temp": null
		},
		"memory": {
			"odi": null,
			"system": null
		},
		"stats": {
			"diskSpace": null,
			"totalLines": null,
			"update": null
		},
		"buttonStats": {
			"ok": 0,
			"cancel": 0,
			"white": 0,
			"blue": 0,
			"blueError": 0
		}
	},
	"cron": {
		"base": [
			{
				"cron": "*/30 * * * * *",
				"flux": {
					"id": "interface|hardware|runtime",
					"conf": {
						"log": "trace"
					}
				}
			},
			{
				"cron": "1 * * * * *",
				"flux": {
					"id": "service|alarm|isAlarm",
					"conf": {
						"log": "trace"
					}
				}
			},
			{
				"cron": "0 2 0 * * 1",
				"flux": {
					"id": "interface|hardware|archiveLog"
				}
			}
		],
		"full": [
			{
				"cron": "5 0 0 * * 1-5",
				"flux": {
					"id": "service|system|goToSleep"
				}
			},
			{
				"cron": "5 0 2 * * 0,6",
				"flux": {
					"id": "service|system|goToSleep"
				}
			},
			{
				"cron": "13 13 13 * * 1-6",
				"flux": [
					{
						"id": "interface|tts|speak",
						"data": {
							"lg": "en",
							"msg": "Auto restart"
						}
					},
					{
						"id": "service|system|restart",
						"conf": {
							"delay": 3
						}
					}
				]
			},
			{
				"cron": "13 13 13 * * 0",
				"flux": [
					{
						"id": "interface|tts|speak",
						"data": {
							"lg": "en",
							"msg": "Reset config"
						}
					},
					{
						"id": "service|context|reset",
						"conf": {
							"delay": 3
						}
					}
				]
			},
			{
				"cron": "30 13 13 * * 0",
				"flux": [
					{
						"id": "interface|tts|speak",
						"data": {
							"lg": "en",
							"msg": "Reboot"
						}
					},
					{
						"id": "service|system|reboot",
						"conf": {
							"delay": 3
						}
					}
				]
			}
		]
	}
}
